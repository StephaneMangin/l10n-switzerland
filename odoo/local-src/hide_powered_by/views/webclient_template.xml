<odoo>

  <!-- remove Powered by Odoo from login page -->
  <template id="login_layout" inherit_id="web.login_layout">
    <t t-if="not disable_database_manager" position="replace">
      <t t-if="not disable_database_manager">
        <a class="" href="/web/database/manager">Manage Databases</a>
      </t>
    </t>
    <a href="https://www.odoo.com" position="replace"/>
  </template>

  <!-- remove Powered by Odoo from left menu -->
  <template id="menu_secondary" inherit_id="web.menu_secondary">
    <div class="o_sub_menu_footer" position="replace"/>
  </template>

</odoo>
