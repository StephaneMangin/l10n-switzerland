<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="company_form_view">
    <field name="name">enfinfidu.res.company.form.inherit</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <xpath expr="//notebook" position="inside">
        <page string="Report Data">
          <group colspan="4">
            <field name="report_logo" widget="image"/>
          </group>
          <group colspan="4">
            <field name="report_partner_bank_id" domain="[('company_id', '=', id)]"/>
          </group>
        </page>
      </xpath>
    </field>
  </record>


</odoo>
